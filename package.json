{
	"name": "github-issues-prs",
	"displayName": "GitHub Issues",
	"version": "0.0.1",
	"private": false,
	"description": "View the issues assigned to you in the Explorer viewlet.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandGitHubIssuesPrs#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandGitHubIssuesPrs/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandGitHubIssuesPrs.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -watch -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "githubIssuesPrs.refresh",
				"icon": {
					"dark": "resources/dark/refresh.svg",
					"light": "resources/light/refresh.svg"
				},
				"title": "Refresh"
			},
			{
				"command": "githubIssuesPrs.createIssue",
				"icon": {
					"dark": "thirdparty/octicons/dark/plus-small.svg",
					"light": "thirdparty/octicons/light/plus-small.svg"
				},
				"title": "Create Issue"
			},
			{
				"command": "githubIssuesPrs.openMilestone",
				"title": "Open Milestone"
			},
			{
				"command": "githubIssuesPrs.openIssue",
				"title": "Open Issue"
			},
			{
				"command": "githubIssuesPrs.openPullRequest",
				"title": "Open Pull Request"
			},
			{
				"command": "githubIssuesPrs.checkoutPullRequest",
				"title": "Checkout Pull Request"
			},
			{
				"command": "githubIssuesPrs.copyNumber",
				"title": "Copy Number"
			},
			{
				"command": "githubIssuesPrs.copyText",
				"title": "Copy Text"
			},
			{
				"command": "githubIssuesPrs.copyMarkdown",
				"title": "Copy Markdown"
			},
			{
				"command": "githubIssuesPrs.copyUrl",
				"title": "Copy Url"
			}
		],
		"configuration": {
			"properties": {
				"github.host": {
					"default": "github.com",
					"description": "The host name to access GitHub. Change this to your GitHub Enterprise host.",
					"scope": "application",
					"type": "string"
				},
				"github.repositories": {
					"default": [],
					"description": "A list of repositories to query for issues.",
					"type": "array"
				},
				"github.username": {
					"default": null,
					"description": "The username to use when accessing GitHub. The default is to consult the Git credential manager.",
					"scope": "application",
					"type": [
						"string",
						"null"
					]
				}
			},
			"title": "GitHub configuration",
			"type": "object"
		},
		"menus": {
			"view/item/context": [
				{
					"command": "githubIssuesPrs.openMilestone",
					"group": "1_navigation",
					"when": "view == githubIssuesPrs && viewItem == milestone"
				},
				{
					"command": "githubIssuesPrs.openIssue",
					"group": "1_navigation",
					"when": "view == githubIssuesPrs && viewItem == issue"
				},
				{
					"command": "githubIssuesPrs.copyNumber",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == issue"
				},
				{
					"command": "githubIssuesPrs.copyText",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == issue"
				},
				{
					"command": "githubIssuesPrs.copyMarkdown",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == issue"
				},
				{
					"command": "githubIssuesPrs.copyUrl",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == issue"
				},
				{
					"command": "githubIssuesPrs.openPullRequest",
					"group": "1_navigation",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				},
				{
					"command": "githubIssuesPrs.checkoutPullRequest",
					"group": "1_navigation",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				},
				{
					"command": "githubIssuesPrs.copyNumber",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				},
				{
					"command": "githubIssuesPrs.copyText",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				},
				{
					"command": "githubIssuesPrs.copyMarkdown",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				},
				{
					"command": "githubIssuesPrs.copyUrl",
					"group": "9_cutcopypaste",
					"when": "view == githubIssuesPrs && viewItem == pull_request"
				}
			],
			"view/title": [
				{
					"command": "githubIssuesPrs.refresh",
					"group": "navigation",
					"when": "view == githubIssuesPrs"
				},
				{
					"command": "githubIssuesPrs.createIssue",
					"group": "navigation",
					"when": "view == githubIssuesPrs"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "githubIssuesPrs",
					"name": "GitHub Issues"
				}
			]
		}
	},
	"activationEvents": [
		"onView:githubIssuesPrs"
	],
	"dependencies": {
		"@octokit/rest": "15.18.1",
		"git-credential-node": "1.1.0",
		"moment": "2.24.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "10.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/icon.png"
}
